/**app.wxss**/
@import "utils/wxParse/wxParse.wxss";

/*  看一下全局配置 */
text {
  font-family: 'zcool-gdh';
  font-size: 24rpx;
  color: #666;
}

/* 隐藏 */

page .hidden {
    font-family: zcool-gdh;
  -webkit-filter: blur(0px); /* Chrome, Opera */
  -moz-filter: blur(0px);
  -ms-filter: blur(0px);
  filter: blur(0px);
  animation: blur 0.3s;
}

input {
  font-family: MicroSoft YaHei;
}

@keyframes blur {
  from {
    -webkit-filter: blur(10px); /* Chrome, Opera */
    -moz-filter: blur(10px);
    -ms-filter: blur(10px);
    filter: blur(10px);
  }

  to {
    -webkit-filter: blur(0px); /* Chrome, Opera */
    -moz-filter: blur(0px);
    -ms-filter: blur(0px);
    filter: blur(0px);
  }
}

/* 隐藏滚动条 */

::-webkit-scrollbar{  
  height: 0;
  width: 0;
  color: transparent;
}

@font-face {
    font-family: 'zcool-gdh';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2');
    font-weight: normal;
    font-style: normal;
}

