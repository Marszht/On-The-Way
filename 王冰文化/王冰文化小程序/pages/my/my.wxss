/* pages/my/my.wxss */
.wrap{
  background:#fafafa;
}
.top{
  display:flex;
  align-items:center;
  font-size:24rpx;
  background:#fff;
  border-bottom:1rpx solid #f0f0f0;
  margin-bottom:14rpx;
  /* justify-content:center; */
}
.top image{
  width:96rpx;
  height:96rpx;
  border-radius:100%;
  margin:12rpx 24rpx;
  float:left;
  background:#eee;
}
.tip{
  background:#fff;
  padding:32rpx;
  font-size:28rpx;
  border-top:1rpx solid #f0f0f0;
  border-bottom:1rpx solid #f0f0f0;
  margin-bottom:14rpx;
}
.none{
  text-align:center;
  margin-top:60rpx;
  font-size:32rpx;
  color:#999;
}
.left{
  float:left;
  width:33rpx;
  height:32rpx;
  margin-top:4rpx;
  margin-right:20rpx;
  background:url(data:image/png;base64,) no-repeat center/cover;
}
.right{
  float:right;
  margin-top:8rpx;
  width:23rpx;
  height:23rpx;
  border-top:1px solid #444;
  border-right:1px solid #444;
  transform:rotateZ(45deg);
}
.icon{
  width:26rpx;
  height:26rpx;
  margin-left:12rpx;
  border-radius:100%;
  background-color:#f54e84;
}
.boy{
  background:url(data:image/png;base64,) no-repeat 24% center/80%,#f54e84;
}
.girl{
  background:url(data:image/png;base64,) no-repeat 24% center/80%,#f54e84;
}