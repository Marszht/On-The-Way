<!-- pages/checkDetail/checkDetail.wxml -->
<scroll-view scroll-y>
    <view class="wrap">
        <view class="checkDetail">
            <block wx:for="{{ checkList }}" wx:key="id">
                <view class="checkDetail_time ">{{ item.trade_time }}</view>
                <view class="checkDetail_income ">{{item.money}}</view>
                <view class="content">
                    <view class="type_income">{{ item.title}}</view>
                    <view class="content_foot">
                        <view class="time">
                            <view class="type_day">{{ item.day }}</view>
                            <view class="type_hour">{{ item.hour }}</view>
                        </view>
                    </view>
                </view>
            </block>
        </view>
    </view>
</scroll-view>