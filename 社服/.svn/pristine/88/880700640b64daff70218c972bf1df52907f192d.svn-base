<view class="wrap">
    <view class="content">
        <view class="income">
            <text class="yes_income">昨天收益 (元)</text>
            <view class="yes_money">{{yesMoney}}</view>
        </view>
        <view class="bill_list">
            <view class="bill_item">
                <text class="item_income">累计收益 (元)</text>
                <view class="item_num">￥{{totalIncome}}</view>
            </view>
            <view class="bill_item">
                <text class="item_income">总金额 (元)</text>
                <view class="item_num">￥{{totalMoney}}</view>
            </view>
            <view class="bill_item">
                <text class="item_income">本月佣金 (元)</text>
                <view class="item_num">￥{{mouthIncome}}</view>
            </view>
        </view>
    </view>
    <!-- 提现 -->
    <navigator url="../my/my" open-type="navigateBack">
        <view class="item">
            <image class="item_icon" src="../../static/img/my/purse.png"></image>
            <text class="text">提现</text>
        </view>
    </navigator>
    <!-- 明细 -->
    <navigator url="../my/my" open-type="navigateBack">
        <view class="item">
            <image class="item_icon" src="../../static/img/my/purse.png"></image>
            <text class="text">明细</text>
        </view>
    </navigator>
</view>