<!-- pages/my/my.wxml -->
<!-- pages/my/my.wxml -->
<!-- 用户信息 -->
<view class="container">
  <view class="user_info">
    <!-- <image src="../../static/img/my/purse.png" /> -->
    <image src="{{avatar}}" class="avatar" />
    <text class="user_nick_name">{{nickName}}</text>
    <!-- 我的订单 -->
    <view class="order_sn">
      <view class="head">
      <text class="order">我的订单</text>
      <text class="see-all">查看全部</text>
      </view>
      <view class="tickets">
        <view class="items" bintap="unpay">
          <view class="number">{{unpayTickets}}</view>
          <text class="txt">未付款</text>
        </view>
        <view class="items" bintap="unuse">
          <view class="number">{{unuseTickets}}</view>
          <text class="txt">未使用</text>
        </view>
        <view class="items" bindtap="using">
          <view class="number">{{uningTickets}}</view>
          <text class="txt">使用中</text>
        </view>
        <view class="items" bindtap="used">
          <view class="number">{{usedTickets}}</view>
          <text class="txt">已使用</text>
        </view>
      </view>
    </view>
  </view>
  <!-- 详细信息 -->
  <view class="user_item">
    <navigator url="../mypurse/purse" open-type="navigate">
      <view class="item">
        <image class="item_icon" src="../../static/img/my/purse.png"></image>
        <text class="content">我的钱包</text>
      </view>
    </navigator>
    <!-- <navigator url="../yongDetail/yongDetail" open-type="navigate">
      <view class="item">
        <image class="item_icon" src="../../static/img/my/yong.png"></image>
        <text class="content">分销佣金</text>
      </view>
    </navigator> -->
    <!-- <navigator url="../personCheck/personCheck" open-type="navigate">
    <view class="item">
      <image class="item_icon" src="../../static/img/my/list.png"></image>
      <text class="content">个人账单</text>
    </view>
    </navigator> -->
    <view class="spacebetween"></view>
    <navigator url="../friends/friends" open-type="navigate">
      <view class="item">
        <image class="item_icon" src="../../static/img/my/friend.png"></image>
        <text class="content">我的好友</text>
      </view>
    </navigator>
    <view class="item">
      <image class="item_icon" src="../../static/img/my/collect.png"></image>
      <text class="content">收藏夹</text>
    </view>
    <navigator url="../shopMap/shopMap" open-type="navigate">
      <view class="item">
        <image class="item_icon" src="../../static/img/my/shop.png"></image>
        <text class="content">店铺地址</text>
      </view>
    </navigator>
  </view>
</view>