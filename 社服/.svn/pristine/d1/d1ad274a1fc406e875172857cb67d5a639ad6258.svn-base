/* components/search/search.wxss */
.search{
  height:100rpx;
  background:#fff;
  margin:1px 0;
  padding:15rpx 24rpx;
  box-sizing:border-box;
  position:relative;
}
.s_nav{
  width:100%;
  height:100%;
  box-sizing:border-box;
  padding:0 25rpx;
  background:#f2f2f2;
  border-radius:100rpx;
  display:flex;
  align-items:center;
}
.icon,.clear{
  width:36rpx;
  height:36rpx;
  padding:4rpx;
  overflow:hidden;
  margin-right:15rpx;
}
.icon image,.clear image{
  width:100%;
  height:100%;
}
.input,.toInput{
  flex:1;
}
.toInput{
  color:#989898;
}
.btn{
  padding-left:36rpx;
  height:40rpx;
  line-height:40rpx;
  border-left:1px solid #b2b2b2;
}
.is_to{
  position:absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
}
